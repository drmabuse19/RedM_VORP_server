-  * ------------------------------------------------------------------------------------------
-  StoreHoursAllowed = true, -- if you want the stores to use opening and closed hours
-  StoreOpen = 7, -- am
-  StoreCLose = 21 -- pm
-  * -----------------------------------------------------------------------------------------
